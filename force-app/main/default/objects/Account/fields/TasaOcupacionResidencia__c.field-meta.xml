<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>TasaOcupacionResidencia__c</fullName>
    <description>Porcentaje de ocupación de las plazas totales de residencia</description>
    <externalId>false</externalId>
    <formula>(PlazasPublicasResidenciaOcupadas__c +  PlazasPrivadasResidenciaOcupadas__c ) /  TotalPlazasRP__c</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>% Ocupación RP</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <type>Percent</type>
</CustomField>
